@model Inmobiliaria.Models.Propietario

@{
    ViewData["Title"] = "Delete";
}




<div class="div_delete">
    <div class="div_delete_contenido">
        <h2>Eliminar Propietario</h2>
        <h4 style="color:red">¿Estas seguro de eliminar?</h4>
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nombre)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nombre)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Apellido)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Apellido)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Dni)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Dni)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Clave)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Clave)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Domicilio)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Domicilio)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Telefono)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Telefono)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Borrar" class="btn btn-danger" /> |
        </form>
    </div>
</div>
